{
  "name": "vvssl",
  "version": "1.0.0",
  "description": "A command line utility to quickly setup https for any Vagrant site",
  "main": "index.js",
  "bin": {
    "vvssl": "bin/vvssl.js"
  },
  "preferGlobal": "true",
  "scripts": {
    "test": "eslint . && jscs ."
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "jscsConfig": {
    "preset": "10up"
  },
  "eslintConfig": {
    "extends": "10up",
    "rules": {
      "no-console": [
        0
      ],
      "no-process-env": [
        0
      ]
    }
  },
  "author": "Jonathan Neal <jonathantneal@hotmail.com> (http://jonathantneal.com)",
  "license": "CC0-1.0",
  "dependencies": {
    "argv": "0.0.2",
    "assign": "^0.1.7",
    "child-process-promise": "^2.0.2",
    "colors": "^1.1.2",
    "findit": "^2.0.0",
    "fs-promise": "^0.5.0",
    "nginx-conf": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "^2.12.0",
    "eslint-config-10up": "github:jonathantneal/eslint-config-10up",
    "jscs": "^3.0.4",
    "jscs-config-10up": "github:jonathantneal/jscs-config-10up"
  }
}
